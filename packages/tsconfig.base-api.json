{
  "extends": "../tsconfig",
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "composite": true
  }
}
